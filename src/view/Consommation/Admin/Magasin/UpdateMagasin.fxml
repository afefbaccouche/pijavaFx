<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Consommation.Admin.Magasin.UpdateMagasinController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="18.0" layoutY="10.0" prefHeight="600.0" prefWidth="800.0" spacing="20.0">
         <children>
            <Label fx:id="headerLabel" styleClass="header" text="modifier Magasin">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="428.0" prefWidth="760.0" spacing="20.0">
               <children>
                  <GridPane prefHeight="268.0" prefWidth="344.0">
                     <children>
                        <Label text="Nom de Magasin" />
                        <Label prefHeight="86.0" prefWidth="177.0" text="address Magasin" GridPane.rowIndex="1" />
                        <Label text="tel de Magsin" GridPane.rowIndex="2" />
                        <TextField fx:id="nomMagsin" GridPane.columnIndex="1" />
                        <TextField fx:id="addressMagasin" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <TextField fx:id="telMagasin" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Label text="mail" GridPane.rowIndex="3" />
                        <TextField fx:id="mailMagasin" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                     </children>
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="177.0" minWidth="10.0" prefWidth="177.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="213.0" minWidth="10.0" prefWidth="167.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                  </GridPane>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0">
                     <children>
                        <ImageView fx:id="imageView" fitHeight="185.0" fitWidth="268.0" pickOnBounds="true" preserveRatio="true" />
                        <Button maxWidth="170.0" minWidth="170.0" mnemonicParsing="false" prefWidth="170.0" text="Change Image" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Button fx:id="btnSaveUpM" mnemonicParsing="false" text="Save" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
      
   </children>
    
   
