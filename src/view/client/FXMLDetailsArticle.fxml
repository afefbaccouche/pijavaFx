<AnchorPane id="AnchorPane" fx:id="root" prefHeight="683.0" prefWidth="1308.0" style="-fx-background-color: #ffffcc;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.client.DetailsArticleController">
   <children>
      <Label fx:id="titreArticle" alignment="CENTER" layoutX="424.0" layoutY="59.0" prefHeight="105.0" prefWidth="431.0" text="titre article" textAlignment="CENTER" textFill="#f20953">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <ImageView fitHeight="35.0" fitWidth="33.0" layoutX="22.0" layoutY="21.0" onMouseClicked="#goBack" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Admin/images/back.png" />
         </image>
      </ImageView>
      <Label fx:id="dateArticle" layoutX="1123.0" layoutY="78.0" prefHeight="42.0" prefWidth="96.0" text="date article" textFill="#370bd7" underline="true">
