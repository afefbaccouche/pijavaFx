            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Label fx:id="descArticle" layoutX="112.0" layoutY="134.0" prefHeight="56.0" prefWidth="567.0" text="description article">
         <font>
            <Font name="System Italic" size="13.0" />
         </font>
      </Label>
      <Label fx:id="bodyArticle" alignment="TOP_CENTER" contentDisplay="CENTER" layoutX="506.0" layoutY="236.0" prefHeight="228.0" prefWidth="719.0" text="body article" textAlignment="CENTER" />
      <ImageView fx:id="likeImg" fitHeight="45.0" fitWidth="85.0" layoutX="129.0" layoutY="565.0" onMouseClicked="#likeArticle" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Admin/images/not_liked.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgArticle" fitHeight="166.0" fitWidth="232.0" layoutX="123.0" layoutY="259.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
